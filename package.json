{
  "name": "connect-phone",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "heroku-postbuild": "turbo run build --filter=./packages/database --filter=./apps/api",
    "dev": "turbo run dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "run-all-local-installs": "cd apps/cms && npm install && cd ../api && npm install && cd ../../packages/database && npm install",
    "cms": "cd apps/cms && node -e require('fs-extra').remove('.next') && turbo run dev",
    "api": "cd apps/api && npm run build && turbo run start",
    "api-watch": "cd apps/api && npm run build && turbo run dev",
    "test-api": "cd apps/api && npm run test",
    "clean-cms": "cd apps/cms && node -e require('fs-extra').remove('.next')",
    "prisma-generate": "cd packages/database && prisma generate",
    "prisma-migrate": "cd packages/database && prisma migrate dev --skip-generate",
    "prisma-reset": "cd packages/database && prisma migrate reset --force",
    "prisma-deploy": "cd packages/database && prisma migrate deploy",
    "prisma-studio": "cd packages/database && npx prisma studio",
    "prisma-update": "cd packages/database && npm i --save-dev prisma@latest @prisma/client@latest",
    "prisma-seed": "cd packages/database/prisma && tsx seed.ts",
    "driz-build": "cd packages/database && npm run build",
    "driz-drop": "cd packages/database && npx drizzle-kit drop",
    "driz-generate": "cd packages/database && npx drizzle-kit generate",
    "driz-migrate": "cd packages/database && npx drizzle-kit migrate",
    "driz-push": "cd packages/database && npx drizzle-kit push",
    "driz-seed": "cd packages/database && tsx seed.ts",
    "db-reset": "cd packages/database && tsx reset-db.ts && npx drizzle-kit drop && npx drizzle-kit generate && npx drizzle-kit migrate && npx drizzle-kit push && npm run build && tsx seed.ts && echo All done!",
    "delete-node-modules": "node -e require('fs-extra').remove('node_modules') && cd apps/cms && node -e require('fs-extra').remove('node_modules') && cd ../api && node -e require('fs-extra').remove('node_modules') && cd ../../packages/database && node -e require('fs-extra').remove('node_modules')",
    "delete-node-modules-api": "cd apps/api && node -e require('fs-extra').remove('node_modules')",
    "run-local-db": "docker-compose up -d",
    "stop-local-db": "docker-compose down",
    "delete-all-builds": "cd apps/api && node -e require('fs-extra').remove('dist') && cd ../cms && node -e require('fs-extra').remove('.next') && cd ../../packages/database && node -e require('fs-extra').remove('dist')"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/multer": "^2.0.0",
    "prettier": "^3.5.0",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.8",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.2.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@changesets/cli": "^2.28.1",
    "@clerk/nextjs": "^6.33.7",
    "@connect-phone/shared-types": "*",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@faker-js/faker": "^9.5.0",
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "@mui/material-nextjs": "^6.4.3",
    "@tanstack/react-query": "^5.66.9",
    "@tanstack/react-query-devtools": "^5.66.9",
    "@toolpad/core": "^0.12.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vercel/blob": "^0.27.3",
    "axios": "^1.8.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.3.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.544.0",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-toastify": "^11.0.5"
  },
  "overrides": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2"
  }
}
