{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "cms#dev": {
      "dependsOn": []
    },
    "api#dev": {
      "dependsOn": []
    }
  },
  "globalEnv": ["DATABASE_URL", "DIRECT_URL"]
}
